{
  "config" : "./config",
  "env" : {
    "development" : {
      "logger" : {
        "level"  : "debug",
        "rotate" : {
          "path" : "./logs",
          "name" : "<%= name %>"
        }
      }
    },
    "staging" : {
      "logger" : {
        "level"  : "info",
        "rotate" : {
          "path" : "./logs",
          "name" : "<%= name %>"
        }
      }
    }, 
    "production" : {
      "logger" : {
        "level"  : "info",
        "rotate" : {
          "path" : "./logs",
          "name" : "<%= name %>"
        }
      }
    }
  }
}